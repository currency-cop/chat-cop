{
  "name": "chat-cop",
  "productName": "Chat Cop",
  "version": "1.0.0",
  "description": "Your new chat assistant for Path of Exile",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "John",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "name": "Chat Cop",
            "iconUrl": "./resources/icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "platforms": [
            "linux"
          ],
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "platforms": [
            "linux"
          ],
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "datatables.net-scroller-se": "^1.5.0",
    "datatables.net-se": "^1.10.18",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "moment": "^2.22.2",
    "nedb": "^1.8.0",
    "sql.js": "^0.5.0",
    "tail": "^1.2.3",
    "tail-file": "^1.3.1"
  },
  "devDependencies": {
    "electron": "2.0.2",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.2",
    "electron-reload": "^1.2.2"
  }
}
